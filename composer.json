{
  "name": "ergebnis/license",
  "description": "Provides an abstraction of an open-source license.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "license"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/license",
  "support": {
    "issues": "https://github.com/ergebnis/license/issues",
    "source": "https://github.com/ergebnis/license"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "ext-filter": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.35.0",
    "ergebnis/data-provider": "^3.0.0",
    "ergebnis/php-cs-fixer-config": "^5.13.0",
    "ergebnis/phpunit-slow-test-detector": "^2.3.0",
    "fakerphp/faker": "^1.23.0",
    "infection/infection": "~0.27.0",
    "phpunit/phpunit": "^10.3.2",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.18.1",
    "symfony/filesystem": "^6.3.1",
    "vimeo/psalm": "^5.15.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\License\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\License\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.1.20"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
