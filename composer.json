{
  "name": "ergebnis/license",
  "description": "Provides an abstraction of an open-source license.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "license"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/license",
  "support": {
    "issues": "https://github.com/ergebnis/license/issues",
    "source": "https://github.com/ergebnis/license"
  },
  "require": {
    "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
    "ext-filter": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.30.2",
    "ergebnis/data-provider": "^1.3.0",
    "ergebnis/php-cs-fixer-config": "^5.3.1",
    "fakerphp/faker": "^1.21.0",
    "infection/infection": "~0.26.19",
    "phpunit/phpunit": "^9.6.5",
    "psalm/plugin-phpunit": "~0.18.4",
    "rector/rector": "~0.15.21",
    "symfony/filesystem": "^6.0.13",
    "vimeo/psalm": "^5.7.7"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\License\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\License\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.0.25"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
