{
  "name": "ergebnis/license",
  "description": "Provides an abstraction of an open-source license.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "license"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/license",
  "support": {
    "issues": "https://github.com/ergebnis/license/issues",
    "source": "https://github.com/ergebnis/license"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-filter": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28.3",
    "ergebnis/data-provider": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "^4.7.0",
    "fakerphp/faker": "^1.20.0",
    "infection/infection": "~0.26.6",
    "phpunit/phpunit": "^9.5.24",
    "psalm/plugin-phpunit": "~0.17.0",
    "symfony/filesystem": "^5.4.12",
    "vimeo/psalm": "^4.27.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\License\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\License\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "7.4.26"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
