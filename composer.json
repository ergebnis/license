{
  "name": "ergebnis/license",
  "description": "Provides an abstraction of an open-source license.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "license"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/license",
  "support": {
    "issues": "https://github.com/ergebnis/license/issues",
    "source": "https://github.com/ergebnis/license"
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-filter": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.20.0",
    "ergebnis/php-cs-fixer-config": "^2.14.0",
    "fakerphp/faker": "^1.17.0",
    "infection/infection": "~0.18.2",
    "phpunit/phpunit": "^8.5.22",
    "psalm/plugin-phpunit": "~0.16.1",
    "symfony/filesystem": "^5.4.0",
    "vimeo/psalm": "^4.17.0"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\License\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\License\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true
    },
    "platform": {
      "php": "7.3.32"
    },
    "preferred-install": "dist",
    "sort-packages": true
  }
}
