{
  "name": "ergebnis/license",
  "description": "Provides an abstraction of an open-source license.",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "license"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/license",
  "support": {
    "issues": "https://github.com/ergebnis/license/issues",
    "source": "https://github.com/ergebnis/license",
    "security": "https://github.com/ergebnis/license/blob/main/.github/SECURITY.md"
  },
  "require": {
    "php": "~7.4.0 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0 || ~8.5.0",
    "ext-filter": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.49.0",
    "ergebnis/data-provider": "^3.6.0",
    "ergebnis/php-cs-fixer-config": "^6.59.0",
    "ergebnis/phpstan-rules": "^2.5.2",
    "ergebnis/phpunit-slow-test-detector": "^2.20.0",
    "fakerphp/faker": "^1.24.1",
    "infection/infection": "~0.26.6",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.12.10",
    "phpstan/phpstan-deprecation-rules": "^1.2.1",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpstan/phpstan-strict-rules": "^1.6.1",
    "phpunit/phpunit": "^9.6.33",
    "rector/rector": "^1.2.10",
    "symfony/filesystem": "^5.4.41"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\License\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\License\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "7.4.33"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.6-dev"
    },
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
